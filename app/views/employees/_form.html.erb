<%= simple_form_for @employee do |f| %>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :middle_name %>
  <%= f.input :address %>
  <%= f.input :city %>
  <%= f.input :state %>  
  <%= f.input :zip_code %>
  <%= f.input :phone1 %>
  <%= f.input :phone2 %>
  
  <%= f.button :submit %>
<% end %>